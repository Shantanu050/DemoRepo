@model IEnumerable<Employee>
<h3>Employee List</h3>
<hr>
<div style="text-align:right;">
<a asp-controller="Emp" asp-action="Create">Add Employee</a>
</div>
<table class="table">
    <thead>
        <th>@Html.DisplayNameFor(e=>e.EmpId)</th>
        <th>@Html.DisplayNameFor(e=>e.Name)</th>
        <th>@Html.DisplayNameFor(e=>e.Dept.DeptName)</th>
        <th>@Html.DisplayNameFor(e=>e.Dept.Location)</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.EmpId</td>
                <td>@item.Name</td>
                <td>@item.Dept.DeptName</td>
                <td>@item.Dept.Location</td>
                <td>
                    <a asp-controller="Emp" asp-action="Display" asp-route-id=item.EmpId>Display</a>
                </td>
            </tr>
        }
    </tbody>
</table>